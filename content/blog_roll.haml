%section.primary_layout.article_layout
    .inner-bounds.block.background-white

        -# Complimentary Featured Articles - Horizontal Layout
        .content-row
            -# Primary Aside Column
            = render 'aside-column-roll-lists'

            %section.primary-content

                %ul.featured-list.roll-list

                    %li.media-object-horizontal-layout
                        .content-row
                            %header
                                %a.text-meta{:href => "#"} Treatment
                                %h3.text-meta-header Does a Raw Food Diet Help Prevent Heart Disease?
                                %p.text-meta-sub.light-text-color
                                    %a{:href => "#"} By Authur Righter
                                    %span.interpunct &#183
                                    December 22, 2013
                                    %span.interpunct &#183
                                    %a.action{:href=>"#", :'data-icon'=>"i"} 888
                        .grid-2-per.major-r
                            .grid-element.image-content
